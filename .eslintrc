{
  "extends": [
    "canonical",
    "canonical/flowtype"
  ],
  "root": true,
  "rules": {
    "linebreak-style": [
      0
    ],
    "import/max-dependencies": [
      0
    ],
    "indent": [
      0
    ],
    "space-before-function-paren": [
      0
    ],
    "no-implicit-coercion": [
      0
    ],
    "id-length": [
      0
    ],
    "lines-around-comment": [
      0
    ],
    "padded-blocks": 0,
    "sort-keys": 0,
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "arrow-body-style": [
      1,
      "as-needed"
    ],
    "line-comment-position": 0,
    "no-inline-comments": 0,
    "no-extra-parens": 0,
    "no-console": 1,
    "babel/object-curly-spacing": 0,
    "no-unused-vars": "warn"
  }
}
